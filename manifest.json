{
	"manifest_version": 2,
	"name": "CafeGrader Exam",
	"description": "CafeGrader examination mode",
	"version": "1.0",
	"permissions": ["storage", "tabs", "*://*/*"],
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	},
	"background":
	{
		"scripts": ["grader.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": {
			"19": "ku_19.png",
			"38": "ku_38.png"
		},
		"default_title": "CafeGrader Examination",
		"default_popup": "popup.html"
    }
}
